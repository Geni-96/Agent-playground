# Stream-Aware Agent Playground Environment Configuration

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379

# LLM Configuration
# OpenAI
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic Claude
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# LLM Rate Limiting
LLM_MIN_INTERVAL_MS=2000

# TTS Configuration
# ElevenLabs
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# ASR Configuration
# Deepgram
DEEPGRAM_API_KEY=your_deepgram_api_key_here

# Azure Speech Services
AZURE_SPEECH_KEY=your_azure_speech_key_here
AZURE_SPEECH_REGION=your_azure_region

# Mediasoup Configuration
MEDIASOUP_SERVER_URL=ws://localhost:5001

# Agent Configuration
MAX_AGENTS=10
MAX_AGENTS_PER_ROOM=5
SPEAKING_TIME_LIMIT=30000

# Azure TTS
AZURE_TTS_KEY=your_azure_tts_key_here
AZURE_TTS_REGION=your_azure_region_here

# PlayHT
PLAYHT_API_KEY=your_playht_api_key_here
PLAYHT_USER_ID=your_playht_user_id_here

# Application Configuration
NODE_ENV=development
PORT=3000

# Logging
LOG_LEVEL=info

# Agent Configuration
MAX_AGENTS=10
DEFAULT_AGENT_TIMEOUT=30000

# Audio Configuration
AUDIO_FORMAT=mp3
AUDIO_SAMPLE_RATE=48000
